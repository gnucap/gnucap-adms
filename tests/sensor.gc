load ./sensor.so

verilog
paramset mydruck drucksensor;
.beta=0.0054;
.Rnull=6.2e3;
.C_waerm=5e-7;
endparamset

mydruck DR2 (nout,pout,ninp,ndd,0);
spice

* Xd nout npout np ndd nss mydruck

Vdd ndd 0 5
vin ninp 0 pulse iv=0 pv=10 rise=1

.list

.print tran v(nodes) T(DR2)
.tran 0 1 1 trace=a
.print dc v(nodes) T(DR2)
.dc vin 1n 10k * 2

.stat notime
.end
