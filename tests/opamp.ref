* note: nodenames case-sensitive 
Vcc ( ndd 0 ) vdd
Vcc1 ( 0 ndd_minus ) vdd
//* note: nodenames case-sensitive 
vdd #() Vcc (.p(ndd),.n(0));
vdd #() Vcc1 (.p(0),.n(ndd_minus));
vsource_sin #(.offset( 0.),.amplitude(ampl),.frequency(freq)) V1 (.p(nin_p),.n(0));
paramset myota opamp;
 .Rf=NA( 10.); .r1=NA( 100.Meg); .f0dB=1e6; .Sr=5e5; .Rout=NA( 75.); .Rinn=NA( 2.G); .Rinp=NA( 2.G); .Rin=NA( 2.Meg); .A0=2e5;
endparmset

// biased by 1V
myota #() OTA1 (.out(nout1),.inp(nin_p),.inn(0),.vddp(ndd),.vddn(ndd_minus),.vref(nvbias));
// unbiased
myota #() OTA0 (.out(nout0),.inp(nin_p),.inn(0),.vddp(ndd),.vddn(ndd_minus));
// bypass paramset
opamp #() OTA2 (.out(nout2),.inp(nin_p),.inn(0),.vddp(ndd),.vddn(ndd_minus));
