load lang_adms.so
load ./cap.so

adms
`include "discipline.h"
`include "constants.h"

module test_cap(vp, vn);
inout vp, vn;
electrical vp, vn;
parameter real c = 0 (*type="instance"*);
    analog begin
        I(vp, vn) <+ c*ddt(V(vp, vn));
    end
endmodule
endadms

paramset admscap test_cap;
endparamset

module cap (n, p)
   admscap #(.c(c)) c1 (n, p);
endmodule


spice
.echo testdata

v1 1 0 pulse iv=1 pv=0 rise=1
Xcap 0 2 cap c=.5
c1 0 3 .5
r1 1 2 1k
r1 1 3 1k

.print dc v(nodes)
.dc
.print tran v(nodes)
.tran 0 10 10 trace=a
.status notime
.end
