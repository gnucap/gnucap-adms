load ./coil.so
* options method=euleronly

verilog
paramset mycoil coil_br;
	.inductance=100m;
endparamset

mycoil ll (nl,0);
spice

V1 n1 0 pulse iv=0 pv=1 rise=1m delay=0
Rb n1 nb .1k
Rm n1 nl .1k

* builtin coil for comparison
Lb nb 0 100m

.print dc v(nodes) i(R*)
.dc
* + trace=i > /tmp/dbg

.list
.print tran v(nodes) i(ll) v(ll) v(lb)
.end
.tran 0 10m .1m trace=n basic
+ trace=i > /tmp/dbgtr
.stat notime
.end
